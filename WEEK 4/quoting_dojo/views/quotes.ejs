<http>
    <head>
      <title>Quoting Dojo</title>
      <link rel="stylesheet" href="stylesheets/style.css">
    </head>
    <body>
        <a href="/">Home</a>
        <h1>Here are the awesome quotes!</h1>
        <% for(const quote of quotes){ %>
            <h3>"<%= quote.quote %>"</h3>
            <p>-<%= quote.name %> @ <%= quote.date %></p>
            <% } %>
    </body>
</http>